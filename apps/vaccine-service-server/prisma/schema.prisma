datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Vaccine {
  batchNumber String?
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  instock     Int?
  updatedAt   DateTime @updatedAt
  vaccineName String?
}

model Medicine {
  createdAt    DateTime @default(now())
  id           String   @id @default(cuid())
  medicineName String?
  updatedAt    DateTime @updatedAt
}

model User {
  createdAt DateTime @default(now())
  email     String?  @unique
  firstName String?
  id        String   @id @default(cuid())
  lastName  String?
  password  String
  roles     Json
  updatedAt DateTime @updatedAt
  username  String   @unique
}
